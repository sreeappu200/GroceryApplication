<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="multibrowsing" parallel="tests">

	<test thread-count="5" name="Chrometest">
	<parameter name="browsers" value="chrome"></parameter>
		<classes>

			<class name="testscripts.LoginTest">
				 <methods>
					<include name="verifyUserwithValidCredentials"></include>
				</methods> 
			</class>
		</classes>
	</test> 
	<test thread-count="5" name="FirefoxTest">
	<parameter name="browsers" value="firefox"></parameter>
		<classes>

			<class name="testscripts.HomeTest">
			<methods>
					<include name="verifyUserisabletoSuccessfullyLoggedoutCredentials"></include>
				</methods> 
			</class>
		</classes>
	</test>
</suite> 
